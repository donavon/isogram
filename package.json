  "dependencies": {
    "array-to-sentence": "^1.0.1",
    "assert-unique": "^1.0.0",
    "chalk": "^1.0.0",
    "ga-loader-snippets": "^1.1.0",
    "ga-tracker-snippet": "^1.0.0",
    "is-var-name": "^1.0.0",
    "minimist": "^1.1.1",
    "sum-up": "^1.0.1"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "browserify": "^10.1.3",
    "each-exec": "^1.0.0",
    "gulp": "^3.8.11",
    "gulp-jscs": "^1.6.0",
