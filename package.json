{
  "name": "isogram",
  "version": "0.5.0",
  "description": "Generate Google Analytics snippet with any 'isogrammic' parameters you like",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "test": "gulp test",
    "test-only": "tape test-*.js",
    "coverage": "istanbul cover -x=bower_components/** test-api.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/isogram",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/isogram/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "snippet",
    "source",
    "script",
    "code",
    "analytics",
    "google analytics",
    "universal analytics",
    "parameter",
    "parameters",
    "word",
    "word play",
    "isogram",
    "nonpattern",
    "browser",
    "client-side",
    "cli",
    "command-line"
  ],
  "dependencies": {
    "array-to-sentence": "^1.0.1",
    "assert-unique": "^1.0.0",
    "chalk": "^1.0.0",
    "ga-loader-snippets": "^1.1.0",
    "ga-tracker-snippet": "^1.0.0",
    "is-var-name": "^1.0.0",
    "minimist": "^1.1.1",
    "sum-up": "^1.0.1"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "browserify": "^9.0.8",
    "each-exec": "^1.0.0",
    "gulp": "^3.8.11",
    "gulp-jscs": "^1.4.0",
    "gulp-jshint": "^1.10.0",
    "gulp-jsonlint": "^1.0.2",
    "gulp-load-plugins": "^0.10.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "jshint-stylish": "^1.0.1",
    "merge-stream": "^0.1.7",
    "require-bower-files": "^2.0.0",
    "rimraf": "^2.3.2",
    "script-equal": "^1.0.1",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "to-camel-case": "^0.2.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98
  }
}
